# BlankLogo API Environment Variables

# Server
PORT=8080
NODE_ENV=development

# Redis (Railway provides this)
REDIS_URL=redis://localhost:6379

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key

# CORS
CORS_ORIGINS=http://localhost:3838,https://blanklogo.com

# R2 Storage (Optional - can use Supabase storage)
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=blanklogo-videos

# Rate Limiting
RATE_LIMIT_REQUESTS=60
RATE_LIMIT_WINDOW_MS=60000
